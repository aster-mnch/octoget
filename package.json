{
  "name": "octoget",
  "private": true,
  "description": "üêô Download git repositories using GItHub GraphQL API.",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "octoget": "./bin/octoget.mjs"
  },
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "dev": "jiti src/dev.ts",
    "build": "unbuild",
    "test": "vitest",
    "check": "biome check",
    "check:write": "biome check --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aster-mnch/octoget.git"
  },
  "keywords": [],
  "author": "aster-mnch <mnch.mai@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aster-mnch/octoget/issues"
  },
  "homepage": "https://github.com/aster-mnch/octoget#readme",
  "dependencies": {
    "@octokit/graphql": "^8.1.1",
    "citty": "^0.1.6",
    "pathe": "^1.1.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^22.9.0",
    "jiti": "^2.4.0",
    "typescript": "^5.6.3",
    "unbuild": "^2.0.0",
    "vitest": "^2.1.5"
  },
  "packageManager": "pnpm@9.13.0",
  "engines": {
    "node": ">=18"
  }
